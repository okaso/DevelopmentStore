<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.development">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/imagen"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/imagen"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditorProyecto"
            android:color="#123456"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"
                ></meta-data>
        </activity>
        <activity
            android:name=".WEB"
            android:parentActivityName=".EditorProyecto">
            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".EditorProyecto"
            ></meta-data>
        </activity>

    </application>

</manifest>